{
  "name": "adventjs",
  "version": "1.0.0",
  "description": "Solutions to AdventJS challenges",
  "main": "index.js",
  "scripts": {
    "lint": "standard",
    "lint-fix": "standard --fix",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest"
  },
  "devDependencies": {
    "@types/jest": "28.1.3",
    "jest": "28.1.1",
    "standard": "17.0.0"
  },
  "eslintConfig": {
    "extends": "./node_modules/standard/eslintrc.json",
    "ignorePatterns": "build",
    "env": {
      "jest": true
    },
    "plugins": [
      "cypress"
    ]
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brandovidal/adventjs.git"
  },
  "keywords": [
    "adventjs",
    "solution",
    "challengues",
    "midudev"
  ],
  "type": "module",
  "author": "Brando Vidal",
  "license": "MIT",
  "homepage": "https://github.com/brandovidal/adventjs#readme",
  "bugs": {
    "url": "https://github.com/brandovidal/adventjs/issues"
  }
